{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "allowSyntheticDefaultImports": true,
      "lib": [
        "esnext",
        "dom"
      ],
      "jsx": "react-jsx",
      "noEmit": false,
      "strict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "moduleResolution": "node",
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true
    },
    "exclude": ["node_modules"]
  }